{
  "workingConditions": {
    "reducedField": {
      "max": 3,
      "min": -3,
      "nvalues": 100,
      "range": "logspace"
    },
    "electronTemperature": {
      "max": 5,
      "min": 0.03,
      "nvalues": 100,
      "range": "linspace"
    },
    "gasPressure": {
      "unit": "Pa",
      "value": 133.32
    },
    "gasTemperature": {
      "unit": "K",
      "value": 300
    },
    "excitationFrequency": {
      "unit": "Hz",
      "value": 0
    },
    "electronDensity": {
      "unit": "m^-3",
      "value": 1e+19
    }
  },
  "electronKinetics": {
    "isOn": true,
    "eedfType": "boltzmann",
    "ionizationOperatorType": "usingSDCS",
    "growthModelType": "temporal",
    "includeEECollisions": false,
    "LXCatFiles": [
      "Nitrogen/N2_LXCat.txt",
      "Nitrogen/N2_rot_LXCat.txt"
    ],
    "gasProperties": {
      "mass": "Databases/masses.txt",
      "fraction": { "N2": 1.0 },
      "harmonicFrequency": "Databases/harmonicFrequencies.txt",
      "anharmonicFrequency": "Databases/anharmonicFrequencies.txt",
      "rotationalConstant": "Databases/rotationalConstants.txt",
      "electricQuadrupoleMoment": "Databases/quadrupoleMoment.txt",
      "OPBParameter": "Databases/OPBParameter.txt"
    },
    "stateProperties": {
      "energy": {
        "states": {
          "N2(X,v=*)": {
            "name": "harmonicOscillatorEnergy",
            "type": "function"
          },
          "N2(X,v=0,J=*)": {
            "name": "rigidRotorEnergy",
            "type": "function"
          }
        }
      },
      "statisticalWeight": {
        "states": {
          "N2(X,v=*)": {
            "type": "constant",
            "value": 1.0
          },
          "N2(X,v=0,J=*)": {
            "name": "rotationalDegeneracy_N2",
            "type": "function"
          }
        }
      },
      "population": {
        "files": [
          "Nitrogen/N2_vibpop.txt"
        ],
        "states": {
          "N2(X)": {
            "type": "constant",
            "value": 1.0
          },
          "N2(X,v=0,J=*)": {
            "arguments": [
              "gasTemperature"
            ],
            "name": "boltzmannPopulation",
            "type": "function"
          }
        }
      }
    },
    "numerics": {
      "energyGrid": {
        "maxEnergy": 1,
        "cellNumber": 1000,
        "smartGrid": {
          "minEedfDecay": 20,
          "maxEedfDecay": 25,
          "updateFactor": 0.05
        }
      },
      "maxPowerBalanceRelError": 1e-09,
      "nonLinearRoutines": {
        "algorithm": "mixingDirectSolutions",
        "maxEedfRelError": 1e-09,
        "mixingParameter": 0.7
      }
    }
  },
  "output": {
    "isOn": true,
    "folder": "N2-default",
    "dataFiles": [
      "log",
      "eedf",
      "swarmParameters",
      "rateCoefficients",
      "powerBalance",
      "lookUpTable"
    ]
  }
}
