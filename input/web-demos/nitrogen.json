{
  "electronKinetics": {
    "LXCatFiles": [
      "https://demo.lxcat.net/api/scat-css/32668033",
      "https://demo.lxcat.net/api/scat-css/32670664"
    ],
    "gasProperties": {
      "fraction": {
        "N2": 1
      },
      "OPBParameter": "Databases/OPBParameter.txt",
      "anharmonicFrequency": "Databases/anharmonicFrequencies.txt",
      "electricQuadrupoleMoment": "Databases/quadrupoleMoment.txt",
      "harmonicFrequency": "Databases/harmonicFrequencies.txt",
      "mass": "Databases/masses.txt",
      "rotationalConstant": "Databases/rotationalConstants.txt"
    },
    "growthModelType": "temporal",
    "numerics": {
      "energyGrid": {
        "cellNumber": 1024,
        "maxEnergy": 1,
        "smartGrid": {
          "maxEedfDecay": 25,
          "minEedfDecay": 15,
          "updateFactor": 0.05
        }
      },
      "maxPowerBalanceRelError": 1e-9,
      "nonLinearRoutines": {
        "algorithm": "mixingDirectSolutions",
        "maxEedfRelError": 1e-9,
        "mixingParameter": 0.7
      }
    },
    "stateProperties": {
      "energy": {
        "states": {
          "N2(X^1S_g^+,v=*)": {
            "name": "harmonicOscillatorEnergy",
            "type": "function"
          },
          "N2(X^1S_g^+,v=0,J=*)": {
            "name": "rigidRotorEnergy",
            "type": "function"
          }
        }
      },
      "population": {
        "states": {
          "N2(X^1S_g^+)": {
            "type": "constant",
            "value": 1
          },
          "N2(X^1S_g^+,v=0)": {
            "type": "constant",
            "value": 1
          },
          "N2(X^1S_g^+,v=0,J=*)": {
            "arguments": [
              "gasTemperature"
            ],
            "name": "boltzmannPopulation",
            "type": "function"
          }
        }
      },
      "statisticalWeight": {
        "states": {
          "N2(X^1S_g^+,v=*)": {
            "type": "constant",
            "value": 1
          },
          "N2(X^1S_g^+,v=0,J=*)": {
            "name": "rotationalDegeneracy_N2",
            "type": "function"
          }
        }
      }
    },
    "eedfType": "boltzmann",
    "includeEECollisions": false,
    "ionizationOperatorType": "usingSDCS",
    "isOn": true
  },
  "workingConditions": {
    "chamberLength": 1,
    "chamberRadius": 1,
    "electronDensity": {
      "unit": "m^-3",
      "value": 10000000000000000000
    },
    "electronTemperature": {
      "unit": "eV",
      "value": 0.03
    },
    "excitationFrequency": {
      "unit": "Hz",
      "value": 0
    },
    "gasPressure": {
      "unit": "Pa",
      "value": 133.32
    },
    "gasTemperature": {
      "unit": "K",
      "value": 300
    },
    "reducedField": {
      "max": 3,
      "min": -3,
      "nvalues": 50,
      "range": "logspace"
    }
  }
}
