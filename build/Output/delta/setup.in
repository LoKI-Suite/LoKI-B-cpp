workingConditions:
  reducedField: 10           
  electronTemperature: 0.03
  excitationFrequency: 0                      
  gasPressure: 133.32                         
  gasTemperature: 300                         
  electronDensity: 1e19                       
  chamberLength: 1.0                          
  chamberRadius: 1.0                          

electronKinetics:
  isOn: true                           
  eedfType: boltzmann                  

  ionizationOperatorType: conservative 
  growthModelType: spatial             
  includeEECollisions: false           
  LXCatFiles:                          
    - Model/Constant_Elastic.txt
    - Model/Delta_Inelastic.txt
  gasProperties:                       
    mass: Databases/masses.txt
    fraction:
      - A = 1
    harmonicFrequency: Databases/harmonicFrequencies.txt
    anharmonicFrequency: Databases/anharmonicFrequencies.txt
    rotationalConstant: Databases/rotationalConstants.txt
    electricQuadrupoleMoment: Databases/quadrupoleMoment.txt
    OPBParameter: Databases/OPBParameter.txt
  stateProperties:                     
    energy:
      - A(gnd) = 1.0
    statisticalWeight:
      - A(gnd) = 1.0
    population:
      - A(gnd) = 1.0
  numerics:                            
    energyGrid:                        
      maxEnergy: 30
      cellNumber: 3000
    maxPowerBalanceRelError: 1e-9      
    nonLinearRoutines:
      algorithm: mixingDirectSolutions 
      mixingParameter: 0.7             
      maxEedfRelError: 1e-9            

gui: 
  isOn: false
  refreshFrequency: 1

output:
  isOn: true
  folder: delta
  dataFiles:
    - eedf
    - swarmParameters
    - rateCoefficients
    - powerBalance
    - lookUpTable
