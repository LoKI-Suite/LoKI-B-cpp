





workingConditions:
  reducedField:  40
  electronTemperature: linspace(0.03, 5, 300) 
  excitationFrequency: 0                      
  gasPressure: 133.32                         
  gasTemperature: 300                         
  electronDensity: 1e19                       
  chamberLength: 1.0                          
  chamberRadius: 1.0                          


electronKinetics:
  isOn: true                           
  eedfType: boltzmann                  

  ionizationOperatorType: usingSDCS    
  growthModelType: spatial             
  includeEECollisions: false           
  LXCatFiles:                          
    - Oxygen/O2_LXCat.txt
    - Oxygen/O2_rot_LXCat.txt





  gasProperties:                       
    mass: Databases/masses.txt
    fraction:
      - O2 = 1
    harmonicFrequency: Databases/harmonicFrequencies.txt
    anharmonicFrequency: Databases/anharmonicFrequencies.txt
    rotationalConstant: Databases/rotationalConstants.txt
    electricQuadrupoleMoment: Databases/quadrupoleMoment.txt
    OPBParameter: Databases/OPBParameter.txt
  stateProperties:                     
    energy:
      - O2(X,v=*) = harmonicOscillatorEnergy
      - O2(X,v=0,J=*) = rigidRotorEnergy  
    statisticalWeight:
      - O2(X,v=*) = 1.0
      - O2(X,v=0,J=*) = rotationalDegeneracy
    population:
      - O2(X) = 1.0
      - O2(X,v=0) = 1.0
      - O2(X,v=0,J=*) = boltzmannPopulation@gasTemperature
  numerics:                            
    energyGrid:                        
      maxEnergy: 40
      cellNumber: 2000




    maxPowerBalanceRelError: 1e-9      
    nonLinearRoutines:
      algorithm: mixingDirectSolutions 
      mixingParameter: 0.7             
      maxEedfRelError: 1e-9            


          

gui: 
  isOn: false
  refreshFrequency: 1


output: 
  isOn: true
  folder: simulation1
  dataFiles:
    - eedf
    - swarmParameters
    - rateCoefficients
    - powerBalance
    - lookUpTable

